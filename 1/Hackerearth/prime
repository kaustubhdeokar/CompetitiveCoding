#include<iostream>
using namespace std;
int main(){
    
    int i,j;
    int n=10;
    int arr[n]={0};
    for(i=0;i<n;i++){
        cout<<arr[i];
    }

    for(i=2;i*i<n;i++){
        for(j=2*i;j<n;j+=i){    
        arr[j]=1;
        }
        cout<<endl;
    }
    for(i=2;i<n;i++){
        cout<<arr[i];
    }

   return 0;
}
